page {
  background-image:url('https://636c-cloud1-6g5wybika29da54a-1305336167.tcb.qcloud.la/pictures/bg26.jpg?sign=c3ba4e6433657c0ae82a184d4faba340&t=1622213038');
  background-size: 100% auto;
  background-position: left bottom;
  overflow: hidden;
