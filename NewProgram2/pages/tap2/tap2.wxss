.return {

  width: 200rpx !important;
  height: 90rpx;
  text-align: center;
  line-height: 45rpx;
  background: transparent;
  border: 4rpx solid black;
  font-weight: 400;
}

.btn {
  margin-top: 36rpx;
  width: 200rpx !important;
  height: 90rpx;
  text-align: center;
  line-height: 45rpx;
  background: transparent;
  border: 4rpx solid black;
  font-weight: 400;
}

.detail-page {
  width: 100%;
  min-height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

page-container {
  background-color: #eee;
}

page-container .img {
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  width: 100%;
  height: 100%;
  justify-self: flex-start;
  background-color: #e9e8e6;
}

page-container .img image {
  width: 100%;
  flex: 30%;
  vertical-align: middle;
}

page-container text {
  flex: 70%;
  padding: 0 5rpx;
  font-size: 34rpx;
  text-align: left;
}

.close {
  width: 200rpx !important;
  height: 90rpx;
  color: #020202;
  line-height: 45rpx;
  margin-top: 20rpx;
  background: #91d4f3;
  border: 4rpx solid black;
  font-weight: 400;
}

.blank {
  padding-top: 60rpx;
}